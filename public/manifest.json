{
  "name": "Swagger-Typescript",
  "description": "Swagger-Typescript",
  "version": "1.0.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "cookies", "webRequestAuthProvider", "webRequest", "management"],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "css": ["style.css"],
    "run_at": "document_end"
  }],
  "content_security_policy": {},
  "web_accessible_resources": [{
    "resources": ["/images/kitten.jpeg"],
    "matches": ["<all_urls>"]
  }]
}